v0x5fe9b1429560_0 .var "done", 0 0;
v0x5fe9b1429620_0 .var "result", 3 0;
v0x5fe9b1429700_0 .net "rst", 0 0, v0x5fe9b142a140_0;  1 drivers
v0x5fe9b14297c0_0 .var "sum_bit", 0 0;
E_0x5fe9b140e3c0 .event posedge, v0x5fe9b1429700_0, v0x5fe9b1429370_0;
    .scope S_0x5fe9b14116a0;
T_0 ;
    %wait E_0x5fe9b140e3c0;
    %load/vec4 v0x5fe9b1429700_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5fe9b14292d0_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x5fe9b1429430_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5fe9b1429620_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5fe9b1429560_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5fe9b1429560_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5fe9b13d5490_0;
    %pad/u 2;
    %load/vec4 v0x5fe9b1429210_0;
    %pad/u 2;
    %add;
    %load/vec4 v0x5fe9b14292d0_0;
    %pad/u 2;
    %add;
    %split/vec4 1;
    %assign/vec4 v0x5fe9b14297c0_0, 0;
    %assign/vec4 v0x5fe9b14292d0_0, 0;
    %load/vec4 v0x5fe9b14297c0_0;
    %ix/load 5, 0, 0;
    %ix/getv 4, v0x5fe9b1429430_0;
    %assign/vec4/off/d v0x5fe9b1429620_0, 4, 5;
    %load/vec4 v0x5fe9b1429430_0;
    %addi 1, 0, 2;
    %assign/vec4 v0x5fe9b1429430_0, 0;
    %load/vec4 v0x5fe9b1429430_0;
    %pad/u 32;
    %cmpi/e 3, 0, 32;
    %jmp/0xz  T_0.4, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5fe9b1429560_0, 0;
T_0.4 ;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5fe9b1411510;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x5fe9b1429e40_0;
    %inv;
    %store/vec4 v0x5fe9b1429e40_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5fe9b1411510;
T_2 ;
    %vpi_call 2 28 "$dumpfile", "serial_adder.vcd" {0 0 0};
    %vpi_call 2 29 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5fe9b1411510 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5fe9b1429e40_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5fe9b142a140_0, 0, 1;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x5fe9b1429a20_0, 0, 4;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x5fe9b1429ca0_0, 0, 4;
    %load/vec4 v0x5fe9b1429a20_0;
    %store/vec4 v0x5fe9b1429ae0_0, 0, 4;
    %load/vec4 v0x5fe9b1429ca0_0;
    %store/vec4 v0x5fe9b1429d60_0, 0, 4;
