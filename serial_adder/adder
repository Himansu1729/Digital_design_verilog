    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5fe9b142a140_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5fe9b1429fb0_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x5fe9b1429fb0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_2.1, 5;
    %load/vec4 v0x5fe9b1429ae0_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x5fe9b1429960_0, 0, 1;
    %load/vec4 v0x5fe9b1429d60_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x5fe9b1429bd0_0, 0, 1;
    %load/vec4 v0x5fe9b1429ae0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %store/vec4 v0x5fe9b1429ae0_0, 0, 4;
    %load/vec4 v0x5fe9b1429d60_0;
