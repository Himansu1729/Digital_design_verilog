    .port_info 2 /INPUT 1 "a";
    .port_info 3 /INPUT 1 "b";
    .port_info 4 /OUTPUT 1 "sum_bit";
    .port_info 5 /OUTPUT 4 "result";
    .port_info 6 /OUTPUT 1 "done";
v0x5fe9b13d5490_0 .net "a", 0 0, v0x5fe9b1429960_0;  1 drivers
v0x5fe9b1429210_0 .net "b", 0 0, v0x5fe9b1429bd0_0;  1 drivers
