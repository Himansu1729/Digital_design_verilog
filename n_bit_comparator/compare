#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5feecb1288f0 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x5feecb13d430_0 .var "A", 15 0;
v0x5feecb13d510_0 .var "B", 15 0;
v0x5feecb13d5e0_0 .net "eq", 0 0, v0x5feecb13d0f0_0;  1 drivers
v0x5feecb13d6e0_0 .net "gt", 0 0, v0x5feecb13d1c0_0;  1 drivers
v0x5feecb13d7b0_0 .net "lt", 0 0, v0x5feecb13d280_0;  1 drivers
S_0x5feecb128a80 .scope module, "DUT" "n_bit_comparator" 2 4, 3 1 0, S_0x5feecb1288f0;
 .timescale 0 0;
    .port_info 0 /INPUT 16 "A";
    .port_info 1 /INPUT 16 "B";
    .port_info 2 /OUTPUT 1 "lt";
    .port_info 3 /OUTPUT 1 "gt";
    .port_info 4 /OUTPUT 1 "eq";
P_0x5feecb128c60 .param/l "word_size" 0 3 2, +C4<00000000000000000000000000010000>;
v0x5feecb0ee490_0 .net "A", 15 0, v0x5feecb13d430_0;  1 drivers
v0x5feecb13d010_0 .net "B", 15 0, v0x5feecb13d510_0;  1 drivers
v0x5feecb13d0f0_0 .var "eq", 0 0;
v0x5feecb13d1c0_0 .var "gt", 0 0;
v0x5feecb13d280_0 .var "lt", 0 0;
E_0x5feecb1273d0 .event anyedge, v0x5feecb0ee490_0, v0x5feecb13d010_0;
    .scope S_0x5feecb128a80;
T_0 ;
    %wait E_0x5feecb1273d0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5feecb13d1c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5feecb13d280_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5feecb13d0f0_0, 0, 1;
    %load/vec4 v0x5feecb13d010_0;
    %load/vec4 v0x5feecb0ee490_0;
    %cmp/u;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5feecb13d1c0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5feecb0ee490_0;
    %load/vec4 v0x5feecb13d010_0;
    %cmp/u;
    %jmp/0xz  T_0.2, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5feecb13d280_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5feecb13d0f0_0, 0, 1;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5feecb1288f0;
T_1 ;
    %vpi_call 2 7 "$dumpfile", "compare.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5feecb1288f0 {0 0 0};
    %vpi_call 2 9 "$monitor", $time, " A =%h, B= =%h, lt=%b, gt = %b, eq=%b", v0x5feecb13d430_0, v0x5feecb13d510_0, v0x5feecb13d7b0_0, v0x5feecb13d6e0_0, v0x5feecb13d5e0_0 {0 0 0};
    %delay 5, 0;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x5feecb13d430_0, 0, 16;
    %pushi/vec4 1, 0, 16;
    %store/vec4 v0x5feecb13d510_0, 0, 16;
    %delay 5, 0;
    %pushi/vec4 65280, 0, 16;
    %store/vec4 v0x5feecb13d430_0, 0, 16;
    %pushi/vec4 65535, 0, 16;
    %store/vec4 v0x5feecb13d510_0, 0, 16;
    %delay 5, 0;
    %pushi/vec4 4097, 0, 16;
    %store/vec4 v0x5feecb13d430_0, 0, 16;
    %pushi/vec4 4097, 0, 16;
    %store/vec4 v0x5feecb13d510_0, 0, 16;
    %delay 5, 0;
    %pushi/vec4 45056, 0, 16;
    %store/vec4 v0x5feecb13d430_0, 0, 16;
    %pushi/vec4 40985, 0, 16;
    %store/vec4 v0x5feecb13d510_0, 0, 16;
    %delay 5, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
