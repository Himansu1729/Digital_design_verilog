#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 10;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x62f2f77eb7b0 .scope module, "half_adder_tb" "half_adder_tb" 2 2;
 .timescale -9 -10;
v0x62f2f77fd100_0 .var "a", 0 0;
v0x62f2f77fd1c0_0 .var "b", 0 0;
v0x62f2f77fd290_0 .net "c", 0 0, L_0x62f2f77fd5c0;  1 drivers
v0x62f2f77fd390_0 .net "s", 0 0, L_0x62f2f77fd460;  1 drivers
S_0x62f2f77eb940 .scope module, "u0_DUT" "half_adder" 2 6, 3 1 0, S_0x62f2f77eb7b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "s";
    .port_info 3 /OUTPUT 1 "c";
L_0x62f2f77fd460 .functor XOR 1, v0x62f2f77fd100_0, v0x62f2f77fd1c0_0, C4<0>, C4<0>;
L_0x62f2f77fd5c0 .functor AND 1, v0x62f2f77fd100_0, v0x62f2f77fd1c0_0, C4<1>, C4<1>;
v0x62f2f77ebb90_0 .net "a", 0 0, v0x62f2f77fd100_0;  1 drivers
v0x62f2f77fce30_0 .net "b", 0 0, v0x62f2f77fd1c0_0;  1 drivers
v0x62f2f77fcef0_0 .net "c", 0 0, L_0x62f2f77fd5c0;  alias, 1 drivers
v0x62f2f77fcf90_0 .net "s", 0 0, L_0x62f2f77fd460;  alias, 1 drivers
    .scope S_0x62f2f77eb7b0;
T_0 ;
    %vpi_call 2 9 "$dumpfile", "test_half_adder.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x62f2f77eb7b0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62f2f77fd100_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62f2f77fd1c0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62f2f77fd1c0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62f2f77fd100_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62f2f77fd1c0_0, 0, 1;
    %delay 50, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
